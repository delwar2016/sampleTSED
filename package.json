{
  "name": "sampletsed",
  "version": "1.0.0",
  "description": "this is practice project",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf '{src,test}/**/*.{js,js.map}'",
    "build": "npm tsc",
    "test": "npm clean && npm test:lint && npm test:coverage",
    "test:unit": "cross-env NODE_ENV=test nyc mocha",
    "test:coverage": "cross-env NODE_ENV=test nyc mocha",
    "test:lint": "tslint --project tsconfig.json",
    "test:lint:fix": "tslint --project tsconfig.json --fix",
    "travis:deploy-once": "travis-deploy-once",
    "travis:coveralls": "nyc report --reporter=text-lcov | coveralls",
    "tsc": "tsc --project tsconfig.json",
    "tsc:w": "tsc --project tsconfig.json -w",
    "start": "nodemon --watch \"src/**/*.ts\" --ignore \"node_modules/**/*\" --exec ts-node src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "docker:build": "npm build && docker-compose build",
    "deploy": "exit 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/delwar2016/sampleTSED.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/delwar2016/sampleTSED/issues"
  },
  "homepage": "https://github.com/delwar2016/sampleTSED#readme",
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.3",
    "@types/http-proxy": "^1.17.4",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.9.3",
    "@types/request-promise": "^4.1.46",
    "@types/sinon": "^7.5.2",
    "@types/sinon-chai": "^3.2.3",
    "@types/supertest": "^2.0.8",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "concurrently": "^5.1.0",
    "mocha": "^7.1.1",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.0",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.1",
    "sinon-chai": "^3.5.0",
    "supertest": "^4.0.2",
    "ts-node": "^8.8.1",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@tsed/common": "^5.44.10",
    "@tsed/core": "^5.65.7",
    "@tsed/di": "^5.44.10",
    "@tsed/swagger": "^5.44.10",
    "@tsed/testing": "^5.44.10",
    "@types/swagger-schema-official": "^2.0.21",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "express": "^4.17.1",
    "method-override": "^3.0.0",
    "node-uuid": "^1.4.8"
  }
}
